<Window x:Class="EMAS.Windows.Dialogue.HistoryWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EMAS.Windows.Dialogue"
        xmlns:ViewModel="clr-namespace:EMAS.ViewModel"
        xmlns:Controls="clr-namespace:EMAS.Controls"
        mc:Ignorable="d"
        Title="HistoryWindow" Height="500" Width="380">
    <Window.DataContext>
        <ViewModel:HistoryVM/>
    </Window.DataContext>
    <Grid>
        <ListBox Name="HistoryListBox" ItemsSource="{Binding History, Mode=OneWay}"/>
    </Grid>
</Window>
