<Window x:Class="EMAS_WPF.Windows.Dialogue.Reservation.ReservationConfirmationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EMAS_WPF.Windows.Dialogue.Reservation"
        xmlns:reservation="clr-namespace:EMAS_WPF.Controls.Reservation"
        mc:Ignorable="d"
        Title="ReservationConfirmationWindow" Height="450" Width="800">
    <Grid>
        <reservation:ReservationConfirmationControl DataContext="{Binding DataContext, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type local:ReservationConfirmationWindow}}}"/>
    </Grid>
</Window>
