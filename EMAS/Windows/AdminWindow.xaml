<Window x:Class="EMAS_WPF.Windows.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EMAS_WPF.Windows" 
        xmlns:Control="clr-namespace:EMAS_WPF.Control"
        mc:Ignorable="d"
        Title="AdminWindow" Height="450" Width="800">
    <Grid>
        <TabControl>
            <TabItem Header="Объекты">
                <Control:LocationControl/>
            </TabItem>
            <TabItem Header="Сотрудники">
                <Control:EmployeeControl/>
            </TabItem>
            <TabItem Header="Активные сессии"/>
        </TabControl>
    </Grid>
</Window>
