        Title="ReservationCreationWindow" Height="450" Width="800">
    <Grid>
        <reservation:ReservationCreationControl DataContext="{Binding DataContext, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type local:ReservationCreationWindow}}}"/>
